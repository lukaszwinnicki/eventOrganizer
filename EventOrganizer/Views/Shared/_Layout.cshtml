<!DOCTYPE html>
<html ng-app="eventOrganizer">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/styles/bootstrap")
    <link href="~/Content/JQuery/jquery-ui-1.10.0.custom.min.css" rel="stylesheet" />
    @Scripts.Render("~/bundles/jquery", "~/bundles/jquery-ui","~/bundles/jquery-val")
    <script src="~/Scripts/Libraries/Bootstrap/bootstrap.js"></script>
    <script src="~/Scripts/Libraries/Angular/angular.js"></script>
    <script src="~/Scripts/Libraries/AngularUI/angular-ui.js"></script>
    <script src="~/Scripts/Libraries/Angular/angular-resource.min.js"></script>
    <script src="~/Scripts/ng-upload.js"></script>
    <!-- HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
      <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->
</head>
    <body>
        @RenderBody()
        @Scripts.Render("~/bundles/app")
        @RenderSection("scripts", required: false)
    </body>
</html>
