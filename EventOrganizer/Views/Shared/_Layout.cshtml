<!DOCTYPE html>
<html ng-app="eventOrganizer">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/styles/bootstrap")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jquery.val")
    <script src="~/Scripts/Libraries/Bootstrap/bootstrap.js"></script>
    <script src="~/Scripts/Libraries/Angular/angular.js"></script>
    <script src="~/Scripts/Libraries/AngularUI/angular-ui.js"></script>
    <script src="~/Scripts/Libraries/Angular/angular-resource.min.js"></script>
    <script src="~/Scripts/App/App.js"></script>
    <!-- HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
      <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->
</head>
<body>
    <div class="container-narrow">
        <div>
            @Html.Partial("_Menu")
            <h3 class="muted">Event organizer</h3>
        </div>
        <hr>
        <div id="body">
            @RenderBody()
        </div>
        <hr>

        <div>
            <p>&copy; Work4Food @DateTime.Now.Date.Year</p>
        </div>

        @RenderSection("scripts", required: false)
    </div>

</body>
</html>
